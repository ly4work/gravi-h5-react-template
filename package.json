{
  "name": "gravi-h5-react-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:local": "cross-env BUILD_ENV=local umi dev",
    "dev:dev": "cross-env UMI_ENV=dev BUILD_ENV=dev umi dev",
    "dev:test": "cross-env UMI_ENV=dev BUILD_ENV=test umi dev",
    "dev:uat": "cross-env UMI_ENV=dev BUILD_ENV=test umi dev",
    "yx-build:dev": "cross-env BUILD_ENV=dev umi build",
    "yx-build:test": "cross-env BUILD_ENV=test umi build",
    "yx-build:uat": "cross-env BUILD_ENV=uat umi build",
    "yx-build:prod": "cross-env BUILD_ENV=prod umi build",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/preset-react": "1.x",
    "axios": "^0.21.1",
    "umi": "^3.4.6",
    "zarm": "^2.9.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.6",
    "dd-app-sdk": "^1.0.0",
    "gj-sso-sdk": "^1.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
